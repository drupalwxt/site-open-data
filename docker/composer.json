{
    "name": "open-data/site-open-data",
    "description": "Project template for Drupal 8 sites built with the WxT distribution.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "symfony/css-selector": "2.8",
        "behat/behat": "3.*@stable",
        "behat/mink": "1.*@stable",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "drupal/console": "~1.0",
        "drupal/drupal-extension": "*",
        "squizlabs/php_codesniffer": "*",
        "drupal/coder": "8.*@stable",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.1.0",
        "sebastian/phpcpd": "*",
        "phpunit/phpunit": "4.8",
        "deployer/deployer": "^4.0",
        "deployer/recipes": "^4.0"
    },
    "require": {
        "composer/installers": "^1.0",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "drush/drush": "8.*@stable",
        "drupalwxt/wxt": "dev-8.x-2.x",
        "drupalwxt/drupal-scripts": "dev-master",
        "drupal/chosen": "2.1",
        "drupal/csv_serialization": "1.0",
        "drupal/facets": "1.0-alpha6",
        "drupal/field_default_token": "1.x-dev",
        "drupal/sarnia": "dev-8.x-1.x",
        "drupal/search_api": "1.0-beta3",
        "drupal/search_api_solr": "1.0-beta1",
        "drupal/wxt_solr": "dev-8.x-1.x",
        "drupal/views_autocomplete_filters": "1.1",
        "drupal/views_data_export": "1.0-alpha2",
        "drupal/views_field_view": "1.0-beta2",
        "drupal/views_templates": "1.0-alpha1",
        "drupal/xls_serialization": "1.0-alpha2",
        "harvesthq/chosen": "1.5.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
          "type": "vcs",
          "url": "http://github.com/sylus/sarnia.git"
        },
        {
          "type": "vcs",
          "url": "http://github.com/drupalwxt/wxt_solr.git"
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "harvesthq/chosen",
                "version" : "v1.5.1",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/harvesthq/chosen-package/archive/v1.5.1.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/harvesthq/chosen-package.git",
                    "type"      : "git",
                    "reference" : "v1.5.1"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/wet-boew",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/wet-boew-cdn/archive/v4.0.23.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/wet-boew-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-wet-boew",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-wet-boew.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-wet-boew"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-base",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-base.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-base"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gc-intranet",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-gc-intranet.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-gc-intranet"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gcwu-fegc",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-gcwu-fegc.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-gcwu-fegc"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-gcweb",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-gcweb.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-gcweb"
                }
            }
        },
        {
            "type"    : "package",
            "package" : {
                "name"    : "wet-boew/theme-ogpl",
                "version" : "v4.0.23",
                "type"    : "drupal-library",
                "dist"    : {
                    "url"  : "https://github.com/wet-boew/themes-cdn/archive/v4.0.23-theme-ogpl.tar.gz",
                    "type" : "tar"
                },
                "source"  : {
                    "url"       : "https://github.com/wet-boew/themes-cdn.git",
                    "type"      : "git",
                    "reference" : "v4.0.23-theme-ogpl"
                }
            }
        }
    ],
    "scripts": {
      "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
      "pre-install-cmd": [
          "DrupalWxT\\WxT\\ScriptHandler::checkComposerVersion"
      ],
      "pre-update-cmd": [
          "DrupalWxT\\WxT\\ScriptHandler::checkComposerVersion"
      ],
      "post-install-cmd": [
          "@composer drupal-scaffold",
          "DrupalWxT\\WxT\\ScriptHandler::deployLibraries"
      ],
      "post-update-cmd": [
          "@composer drupal-scaffold",
          "DrupalWxT\\WxT\\ScriptHandler::deployLibraries"
      ]
    },
    "config": {
        "secure-http": false,
        "prestissimo": {
            "maxConnections": 10,
            "minConnections": 5
        }
    },
    "autoload": {
        "classmap": [
            "scripts/ScriptHandler.php"
        ]
    },
    "extra": {
        "installer-paths": {
            "html/core": [
                "type:drupal-core"
            ],
            "html/libraries/{$name}" : [
                "type:drupal-library"
            ],
            "html/modules/custom/{$name}": [
                "drupal/wxt_library"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/custom/{$name}": [
                "drupal/wxt_bootstrap"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "scripts/{$name}": [
                "drupalwxt/drupal-scripts"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patches": {
            "deployer/recipes": {
                "Enter deployer/recipes patch #80 description here":
                "https://patch-diff.githubusercontent.com/raw/deployphp/recipes/pull/80.patch"
            },
            "drupal/facets": {
                "Enter drupal/facets patch #2611812 description here":
                "https://www.drupal.org/files/issues/create_a_date_query_type-2611812-45.patch"
            },
            "drupal/chosen": {
                "Enter drupal/chosen patch #2706433 description here":
                "https://www.drupal.org/files/issues/composer_dist_problem-2706433-28.patch"
            },
            "drupal/views_autocomplete_filters": {
                "Enter drupal/views_autocomplete_filters patch #2683339 description here":
                "https://gist.githubusercontent.com/sylus/7d2e6bf4a0e83183908fd03196bfc0cf/raw/51e4ca14d69d60d75460290dc480261831db240b/doesn_t_work_on_sku_field-2683339-6.patch"
            }
        }
    }
}
